{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "semi": "off",
    "comma-dangle": "off",
    "func-names": "off",
    "no-underscore-dangle": "off",
    "no-restricted-syntax": "off"
  }
}
